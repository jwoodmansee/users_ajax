<div class="container">
  <h1>Welcome Users of Ajax</h1>

  <hr />

  <button id='load_users'>View Users</button>
  <button id='create_users'>Create User</button>

  <div id='new_user_content' class='no-display'>
    <form id='new_user' action='/users' method='POST'>
      <input id='first_name' type='text' placeholder='First Name' name='user[first_name]' required />  
      <br />
      <input id='last_name' type='text' placeholder='Last Name' name='user[last_name]' required />
      <br />
      <input id='phone_number' type='text' placeholder='Phone Number' name='user[phone_number]' />
      <br />
      <input type='submit' />
    </form>
  </div>
  <ol id='users'></ol>
</div>  

